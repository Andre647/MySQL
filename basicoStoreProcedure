CREATE PROCEDURE `sp_Exerc01` () /* N ESQUECER DE USAR O DELIMITADOR, ESTE CODIGO SOZINHO NÃO RODA,FOI FEITO PELA INTERFACE DO MY WORKBENCH */
BEGIN
DECLARE CLIENTE VARCHAR(10);
DECLARE IDADE INTEGER;
DECLARE DATA_NASCIMENTO DATE;
DECLARE CUSTO FLOAT;

SET CLIENTE = 'João';
SET IDADE = 10;
SET DATA_NASCIMENTO = '2007-01-10';
SET CUSTO = 10.23;

SELECT CLIENTE;
SELECT IDADE;
SELECT DATA_NASCIMENTO;
SELECT CUSTO;
END
