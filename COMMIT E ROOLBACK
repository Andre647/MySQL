START transaction; /* COMECA A TRANSAÇÃO, SEMPRE TEM QUE COMECAR OUTRA SE QUISER DAR OUTRO ROOLBACK/COMMIT */
SELECT * FROM VENDEDORES;
rollback; /* VOLTA TD PRA ANTES TRANSAÇÃO */
commit; /* EFETUA TUDO Q VC MUDOU */

INSERT INTO `vendas_sucos`.`vendedores`
(`MATRICULA`,
`NOME`,
`BAIRRO`,
`COMISSAO`,
`DATA_ADMISSAO`,
`FERIAS`)
